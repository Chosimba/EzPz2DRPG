        <Button x:Name="button" Content="Start Servers" HorizontalAlignment="Left" Margin="10,184,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Rectangle Fill="#FFF4F4F5" Margin="10,209,10,10" Stroke="Black"/>
        <DataGrid x:Name="dataGrid" Margin="10,209,10,10"/>
        <Button x:Name="button1" Content="Launch Client" Margin="411,184,10,0" VerticalAlignment="Top" Click="button1_Click"/>
        <Grid x:Name="CharDisplay" HorizontalAlignment="Left" Height="169" Margin="10,10,0,0" VerticalAlignment="Top" Width="497"/>
